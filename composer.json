{
  "autoload": {
    "psr-4": {
      "CoffeeManager\\": "src/CoffeeManager"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CoffeeManager\\Tests\\Unit\\": "tests/Unit/CoffeeManager",
      "CoffeeManager\\Tests\\Integration\\": "tests/Integration/CoffeeManager"
    }
  },
  "require": {
    "php": "^8.2",
    "symfony/console": "^6.4",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "infection/infection": "^0.26.6",
    "phpstan/phpstan": "^1.10"
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
